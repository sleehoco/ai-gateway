# Environment Variables Template
# Copy this file to .env and update with your actual values

# Database Configuration
DATABASE_URL=postgresql://ai_gateway:your_secure_password@postgres:5432/ai_gateway
POSTGRES_PASSWORD=your_secure_postgres_password

# LiteLLM Configuration
LITELLM_MASTER_KEY=sk-your-secure-master-key-here-change-this
LITELLM_SALT_KEY=your-salt-key-for-hashing-change-this

# Local Ollama Configuration
OLLAMA_API_BASE=http://192.168.1.100:11434

# Cloud Provider API Keys (add your actual keys)
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key
GROQ_API_KEY=gsk_your-groq-api-key
GEMINI_API_KEY=your-gemini-api-key
TOGETHER_AI_API_KEY=your-together-ai-api-key

# Backend Configuration
JWT_SECRET=your-jwt-secret-key-change-this
MAX_USERS=5
CORS_ORIGIN=http://localhost:3002

# Frontend Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_WS_URL=ws://localhost:3001
REACT_APP_MAX_USERS=5
APP_VERSION=1.0.0

# Monitoring Configuration
GRAFANA_PASSWORD=your_grafana_password_change_this

# Optional: Development Overrides
NODE_ENV=production
LOG_LEVEL=info

# Optional: Cloudflare Configuration (if using)
CLOUDFLARE_TUNNEL_TOKEN=your-cloudflare-token
DOMAIN_NAME=your-domain.com